def kuorani(x,y,z):    global kullanabilirlik    kullanabilirlik=(x-y)/z    return kullanabilirlikdef porani(a,b,c,d):    global performans    performans=(a*b)/(c-d)    return performansdef korani(e,f):    kalite=e/f    return kalitedef OEE(kullanabilirlik,performans,kalite):    OEE=kullanabilirlik*performans*kalite    if(OEE<75):        print("oee düşükk")    elif(OEE==75):            print("oee eşit")    else:        print("oee fazla")x=int(input("Planlanmış üretim süresi"))y=int(input("plansız duruş giriniz"))z=int(input("planlanmış üretim süresi"))a=int(input("standart çevrim zamanı giriniz"))b=int(input("üretim miktarı giriniz"))c=int(input("planlanmış üretim süresi"))d=int(input("plansız duruş giriniz"))e=int(input("sağlam ürün miktarı"))f=int(input("toplam üretim miktarı"))kullanabilirlik=kuorani(x,y,z)performans=porani(a,b,c,d)kalite=korani(e,f)OEE(kullanabilirlik,performans,kalite)
